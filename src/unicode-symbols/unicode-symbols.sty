% These allow chaining of unicode subscripts, e.g. X\subscript{1}\subcsript{2} is identical to Xâ‚â‚‚

\NewDocumentCommand{\safe@strip@prefix}{m}{%
	%
	% Safe version of the strip@prefix command
	% If the input is empty, it returns empty
	%
	\edef\@temp{\expandafter\strip@prefix#1>}%
	% The temp is <<\meaning\temp>>%
	% check if \temp is empty
	\ifx\@temp\@empty%
		#1%
	\else%
		\expandafter\strip@prefix#1%
	\fi
}

\NewDocumentCommand{\umeaning}{m}{%
	%
	% Prints the meaning of a character
	%
	\edef\unicode@meaning@temp{\detokenize{#1}}%
	% check if the result is empty
	\ifx\unicode@meaning@temp\empty%
		---empty---%
	\else%
		\ifPDFTeX%
			% check if the result defines a csname
			\ifcsname u8:\unicode@meaning@temp\endcsname%
				\expandafter\expandafter\expandafter\strip@prefix%
				\expandafter\meaning\csname u8:\unicode@meaning@temp\endcsname%
			\else%
				\expandafter\meaning\unicode@meaning@temp\relax%
			\fi%
		\else%
			\safe@strip@prefix{\meaning#1}%
		\fi%
	\fi%
}%

\NewDocumentCommand\PrintMathFonts{}{%
	%
	% Displays the used math fonts
